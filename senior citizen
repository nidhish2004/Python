p=int(input(&quot;Enter the Principle amount:&quot;))
n=int(input(&quot;Enter the number of years:&quot;))
SC=input(&quot;Senior Citizen Yes/No:&quot;)
G=input(&quot;Male/Female:&quot;)
if SC==&#39;Y&#39; and G==&#39;M&#39;:
print(&quot;SI=&quot;,(p*n*12)/100)
elif SC==&#39;Y&#39; and G==&#39;F&#39;:
print(&quot;SI=&quot;,(p*n*15)/100)
else:

print(&quot;SI=&quot;,(p*n*10)/100)
