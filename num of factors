x=int(input(&quot;Enter the number:&quot;))
y=[]
print(&quot;The factors of&quot;,x,&quot;are:&quot;)
for i in range(1, x):
if x % i == 0:
y.append(i)
print(y)
print(&quot;Number of factors:&quot;, len(y))
n=int(input(&quot;Enter N value:&quot;))
if n&gt;len(y):
print(&quot;Invalid&quot;)
else:
print(&quot;First&quot;, n, &quot;factors:&quot;)
for k in range(0,n):
print(y[k], end=&#39; &#39;)
